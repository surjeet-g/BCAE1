    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.1",
    "@timwangdev/react-native-geocoder": "^1.0.0-alpha.7",
    "axios": "^0.27.2",
    "better-docs": "^2.7.2",
    "lodash.get": "^4.4.2",
