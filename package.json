{
  "name": "BCAE_App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docs": "jsdoc -c jsdoc.conf.json",
    "ios:img-fix": "npx react-native-fix-image",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "ios_build": "npx  react-native run-ios --configuration=release",
    "pod_install_m1": "cd ios && arch -x86_64 pod install",
    "build:ios": "npx react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:pre": "rm -rf ./ios/Pods && rm -rf ./ios/Podfile.lock && rm -rf ./ios/BCAE_App.xcworkspace && rm -rf ~/Library/Developer/Xcode/DerivedData/* && npm run pod_install_m1 && npm run ios:img-fix && npm run build:ios && cd ./ios/ && xcrun agvtool next-version -all &&  cd ../   && npm run ios"
  },
  "dependencies": {
    "@babel/traverse": "^7.19.1",
    "@react-native-community/async-storage": "^1.11.0-alpha.0",
